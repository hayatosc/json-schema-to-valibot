{
  "name": "json-schema-to-valibot",
  "type": "module",
  "version": "0.1.1",
  "description": "Convert JSON Schema definitions to Valibot schema definitions. Inspired by json-schema-to-zod.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "json-schema-to-valibot": "dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rolldown --config rolldown.config.mjs && tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "tsx src/cli.ts",
    "test": "pnpm test:dev && pnpm test:e2e && pnpm test:suite",
    "test:dev": "vitest --exclude='**/end-to-end.test.ts'",
    "test:e2e": "pnpm build && vitest end-to-end.test.ts",
    "test:suite": "pnpm build && tsx script/test-suite-runner.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "json-schema",
    "valibot",
    "schema",
    "validation",
    "converter",
    "cli",
    "typescript"
  ],
  "author": "hayatosc",
  "license": "MIT",
  "dependencies": {
    "citty": "^0.1.6",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.18.0",
    "rolldown": "^0.15.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
