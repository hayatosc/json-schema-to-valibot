{
  "name": "json-schema-to-valibot",
  "type": "module",
  "version": "0.1.0",
  "description": "Convert JSON Schema definitions to Valibot schema definitions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "json-schema-to-valibot": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rolldown --config rolldown.config.mjs",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "json-schema",
    "valibot",
    "schema",
    "validation",
    "converter",
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "citty": "^0.1.6",
    "valibot": "^0.42.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.18.0",
    "rolldown": "^0.15.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}